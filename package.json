{
  "name": "hs-deck-builder",
  "version": "1.0.0",
  "description": "hs-deck-builder ---",
  "private": true,
  "main": "index.js",
  "scripts": {
    "compile": "$(npm bin)/tsc --noEmit",
    "dev": "$(npm bin)/babel-node $(npm bin)/webpack-dev-server --config ./client/webpack/dev.config.js",
    "build": "$(npm bin)/babel-node $(npm bin)/webpack --config ./client/webpack/prod.config.js",
    "lint": "$(npm bin)/eslint client/src/* --ext .ts,.tsx",
    "lint:fix": "$(npm bin)/eslint client/src/* --ext .ts,.tsx --fix",
    "stylelint": "$(npm bin)/stylelint 'client/src/**/*.scss'",
    "stylelint:fix": "$(npm bin)/stylelint 'client/src/**/*.scss' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakai-akinobu/hs-deck-builder.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint && npm run compile"
    }
  },
  "keywords": [
    "hearthstone"
  ],
  "author": "sakai-akinobu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sakai-akinobu/hs-deck-builder/issues"
  },
  "homepage": "https://github.com/sakai-akinobu/hs-deck-builder#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-icons": "^2.2.6",
    "@types/react-redux": "^7.0.1",
    "@types/redux-actions": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-hooks": "^1.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "postcss-loader": "^2.1.6",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-prettier": "^1.0.6",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "deckstrings": "^2.1.0",
    "hs-standard-sets": "^2.2.3",
    "immer": "^1.5.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-icons": "^2.2.7",
    "react-redux": "^6.0.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-promise": "^0.6.0"
  }
}
